{
    "name": "filemagic",
    "workspaces": [
        "wat_viewer",
        "hex_viewer",
        "archive",
        "classyshark",
        "binutils",
        "binaryxml",
        "imagemagick",
        "cheerpj",
        "ffmpeg",
        "markdown",
        "mhtml",
        "classfile",
        "browser",
        "dexviewer",
        "textviewer",
        "main"
    ],
    "scripts": {
        "copy-index": "mkdir -p dist && cp index.html dist/index.html",
        "run-server": "node serve.mjs",
        "watch:main": "npm run -w main watch",
        "watch:wat_viewer": "npm run -w wat_viewer watch",
        "watch:hex_viewer": "npm run -w hex_viewer watch",
        "watch:archive": "npm run -w archive watch",
        "watch:classyshark": "npm run -w classyshark watch",
        "watch:binutils": "npm run -w binutils watch",
        "watch:binaryxml": "npm run -w binaryxml watch",
        "watch:imagemagick": "npm run -w imagemagick watch",
        "watch:cheerpj": "npm run -w cheerpj watch",
        "watch:ffmpeg": "npm run -w ffmpeg watch",
        "watch:markdown": "npm run -w markdown watch",
        "watch:mhtml": "npm run -w mhtml watch",
        "watch:classfile": "npm run -w classfile watch",
        "watch:browser": "npm run -w browser watch",
        "watch:dexviewer": "npm run -w dexviewer watch",
        "watch:textviewer": "npm run -w textviewer watch",
        "serve": "run-p copy-index watch:* run-server"
    },
    "devDependencies": {
        "@types/finalhandler": "^1.2.3",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@types/serve-static": "^1.15.7",
        "esbuild": "^0.23.1",
        "esbuild-plugin-copy": "^2.1.1",
        "esbuild-plugin-wasm": "^1.1.0",
        "finalhandler": "^1.3.1",
        "npm-run-all": "^4.1.5",
        "serve-static": "^1.16.2",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-syntax-highlighter": "^15.6.1",
        "wabt": "^1.0.36"
    },
    "license": "MIT",
    "description": ""
}
