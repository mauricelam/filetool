{
    "name": "filemagic",
    "workspaces": [
        "wat_viewer",
        "hex_viewer",
        "archive",
        "classyshark",
        "binutils",
        "binaryxml",
        "imagemagick",
        "cheerpj",
        "ffmpeg",
        "markdown",
        "mhtml",
        "classfile",
        "browser",
        "dexviewer",
        "textviewer",
        "rustdexviewer",
        "main",
        "webgl_previewer",
        "jqviewer",
        "der"
    ],
    "scripts": {
        "copy-index": "mkdir -p dist && cp index.html dist/index.html",
        "run-server": "node serve.mjs",
        "watch:main": "npm run -w main watch",
        "build:main": "npm run -w main build",
        "watch:wat_viewer": "npm run -w wat_viewer watch",
        "build:wat_viewer": "npm run -w wat_viewer build",
        "watch:hex_viewer": "npm run -w hex_viewer watch",
        "build:hex_viewer": "npm run -w hex_viewer build",
        "watch:archive": "npm run -w archive watch",
        "build:archive": "npm run -w archive build",
        "watch:classyshark": "npm run -w classyshark watch",
        "build:classyshark": "npm run -w classyshark build",
        "watch:binutils": "npm run -w binutils watch",
        "build:binutils": "npm run -w binutils build",
        "watch:binaryxml": "npm run -w binaryxml watch",
        "build:binaryxml": "npm run -w binaryxml build",
        "watch:imagemagick": "npm run -w imagemagick watch",
        "build:imagemagick": "npm run -w imagemagick build",
        "watch:cheerpj": "npm run -w cheerpj watch",
        "build:cheerpj": "npm run -w cheerpj build",
        "watch:ffmpeg": "npm run -w ffmpeg watch",
        "build:ffmpeg": "npm run -w ffmpeg build",
        "watch:markdown": "npm run -w markdown watch",
        "build:markdown": "npm run -w markdown build",
        "watch:mhtml": "npm run -w mhtml watch",
        "build:mhtml": "npm run -w mhtml build",
        "watch:classfile": "npm run -w classfile watch",
        "build:classfile": "npm run -w classfile build",
        "watch:browser": "npm run -w browser watch",
        "build:browser": "npm run -w browser build",
        "watch:dexviewer": "npm run -w dexviewer watch",
        "build:dexviewer": "npm run -w dexviewer build",
        "watch:textviewer": "npm run -w textviewer watch",
        "build:textviewer": "npm run -w textviewer build",
        "watch:rustdexviewer": "npm run -w rustdexviewer watch",
        "build:webgl_previewer": "npm run -w webgl_previewer build",
        "watch:webgl_previewer": "npm run -w webgl_previewer watch",
        "build:rustdexviewer": "npm run -w rustdexviewer build",
        "watch:jqviewer": "npm run -w jqviewer watch",
        "build:jqviewer": "npm run -w jqviewer build",
        "serve": "npm run copy-index && run-p watch:* run-server",
        "build": "npm run copy-index && run-p build:*"
    },
    "devDependencies": {
        "@types/finalhandler": "^1.2.3",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@types/serve-static": "^1.15.7",
        "esbuild": "^0.23.1",
        "esbuild-plugin-copy": "^2.1.1",
        "esbuild-plugin-wasm": "^1.1.0",
        "finalhandler": "^1.3.1",
        "npm-run-all": "^4.1.5",
        "serve-static": "^1.16.2",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "jq-wasm": "^1.1.0-jq-1.7.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-syntax-highlighter": "^15.6.1",
        "wabt": "^1.0.36"
    },
    "license": "MIT",
    "description": ""
}
